Ccompound {
main = text+

text =
  | applySyntactic<Block>
  | (~"{" ~"}" any)

Block = "{" Statement* "}"

Statement =
  | Block
  | Cstatement

Cstatement =
  | NextArgNoSp MaybeSp3 -- nextargMaybeSp
  | NextArgNoSp -- nextargNoSp
  | (~";" any)* ";" -- other

NextArgNoSp = "argc" "--" ";" "argv" "++" ";"
MaybeSp3 = "if" "(" "argc" ">" "0" ")" PutSpace ";"
sp = "'" " " "'"
PutSpace = "putchar" "(" sp ")" ";"
}
